/**********************************
* 公共样式 Start
* mini - small - middle - big - vbig - large - vlarge - huge - vhuge - normal
**********************************/

html{padding:0px;margin:0px;overflow-y:scroll; background-color:#2e74b9;_background-image:url('../css/blank.txt');_background-attachment:fixed;_zoom:expression(function(ele){ele.style.zoom = "1";document.execCommand("BackgroundImageCache", false, true);}(this)); }
body{padding: 0px;margin: 0px;line-height:22px; color:#333;font-size:12px; font-family:tahoma,arial,\5b8b\4f53;}
form{padding: 0px;margin: 0px;}
form{padding:0px;margin:0px;}
img{border:0px;vertical-align:middle;}
a{text-decoration: none;color: #1082AC;}
a:hover{text-decoration:none; color:#f60;}

.clear:after { content:'\20'; display:block; height:0; clear:both; display:block;}
.clear { *zoom:1; overflow:hidden;}

/**********************************
* 公共样式 End
**********************************/

.page-head{height:105px;background:url('../img/top-left.jpg') no-repeat left top}
.index-logo{padding-left:50px;height:70px;}
.control-panel-link{text-align:right;color:#bad8f6;height:30px;padding-right:100px;padding-top:5px}
.control-panel-link-nav{display:inline-block;color:#bad8f6;padding:0px 10px;height:21px;background:url('../img/nav-bg.png') no-repeat right center}
.head-line-bottom{width:100%;height:0px;line-height:0px;border-bottom: 1px solid #99bbdd;border-top: 1px solid #03558f;}
.page-body{padding:5px;}
.index-nav{width:218px;border:1px solid #265f98;background-color:#368dca;}
.page-menu-header{min-height:50px;background:url('../img/control-menu-header.jpg') no-repeat left top;}
.page-menu-content{min-height:480px;padding-top:5px;background:url('../img/control-menu-bg.jpg') no-repeat left top;}
.now dt{border:1px solid red}
.now dd{display:block !important}
.page-menu-con dl dd{display:none;}
.index-close{width:4px;height:100%;}
.btn-close-frame{display:block;width:4px;height:37px;background:url('../img/btn-close-frame.png') no-repeat;}
.btn-open-frame{display:block;width:4px;height:37px;background:url('../img/btn-open-frame.png') no-repeat;}

.index-content{width:100%;height:100%}
.control-contrian{min-height:550px;background:#fff;padding:5px 10px 10px 10px;}
.tag-nav{height:33px;}
.tag-nav div{float:left;padding-right:5px}
.tag-nav-unselect a{display:inline-block;color:#fff;text-align:center;line-height:32px;width:123px;height:33px;background:url('../img/tab-bg.png') no-repeat -126px 0px;}
.tag-nav-select a{display:inline-block;color:#000;font-weight:700;text-align:center;line-height:32px;width:123px;height:33px;background:url('../img/tab-bg.png') no-repeat 0px 0px;}

.index-section{padding:30px 0px 20px 100px;}
.index-section-list{padding:5px 0px;}
.index-section-list label{float:left;text-align:right;width:100px;}
.index-section-list input{vertical-align:middle;}
.index-section-list select{padding:2px;border:1px solid #bed4df;width:180px;color:#7d7d7d;vertical-align:middle;background:url('../img/input-bg.png') no-repeat left top}
.index-section-list textarea{padding:2px;border:1px solid #bed4df;width:370px;color:#7d7d7d;vertical-align:middle;background:url('../img/input-bg.png') no-repeat left top}

.btn-submit{width:96px;height:31px;color:#fff;font-weight:700;text-align:center;font-size:14px;border:none;cursor:pointer;background:url('../img/btn-submit.png') no-repeat;}
.dir-input{padding:2px;border:1px solid #bed4df;width:180px;color:#7d7d7d;background:url('../img/input-bg.png') no-repeat left top}
.dir-big-input{padding:2px;border:1px solid #bed4df;width:370px;color:#7d7d7d;background:url('../img/input-bg.png') no-repeat left top}

.content-box{}
.cp-bottom{padding-top:10px;text-align:center}
.btn-top{display:inline-block;text-align:center;color:#000;width:62px;height:25px;background:url('../img/btn_gray.png') no-repeat}

.p-box{border:1px solid #97bcd6}
.p-box h3{margin:0px;padding:0px;height:32px;color:#fff;padding-left:15px;line-height:30px;background:url('../img/p-box-title.png') repeat-x}
.p-box-con{padding:10px;}

/*	左边导航1-start
==================================*/
.menulist,.menulist02{*zoom:1;}
.menulist h2,.menulist02 h2{ background:url(../img/tag-panel-close-bg.png) no-repeat; display:block; color:#fff;padding-left:50px; line-height:32px; font-weight:bold; height:28px; font-size:14px; margin:0; cursor:pointer;}
.menulist div.panel_add h2{cursor:pointer;}
.menulist div.panel_min h2{background:url(../img/tag-panel-open-bg.png) no-repeat;cursor:pointer;color:#000;}
.menulist .panel_add .sub,.menulist .panel_min .sub,.menulist02 .panel_min .sub{ display:none; padding-top:11px; padding-bottom:12px;}
.menulist .panel_min .sub,.menulist02 .panel_min .sub{display:block;}
.menulist .sub a,.menulist02 .sub a{ display:block; line-height:24px;text-decoration:none; color:#bad8f6; height:23px;padding-left:22px;background:url(../img/icon-tag-title.png) no-repeat left center}
.menulist .sub a:hover,.menulist02 .sub a:hover{color:#fff}
.menulist .sub a.now,.menulist02 .sub a.now{text-decoration:none; background:url(../img/icon-tag-title.png) no-repeat left center; color:#FFF;}
.menulist .sub a.out,.menulist02 .sub a.out{background:url(../img/garr_over.gif) no-repeat #dee7ed;}
.menulist_direct{ margin-top:2px; padding-top:2px; /*background:url(../img/left_line.gif) top left no-repeat;*/}
.menulist02 h2{background:url(../img/left_title_bg02.gif) no-repeat;}
.menulist .sub{margin:0px 0px 0px 50px;}

.sub ul{margin:0px;padding:0px;list-style:none}
.sub ul li{margin:0px;padding:0px;}
.sub-2 li a{margin-left:20px;}
/*	左边导航1-end
==================================*/
.com-con-title{margin:0px;padding:0px;font-size:14px;padding-bottom:5px;border-bottom:1px solid #ccc;margin-bottom:10px;}

.i_account_list{width:100%; separate; border-bottom:1px solid #ccc;}
.i_account_list td{border-right:1px solid #ccc;padding:5px 0px 5px 5px;border-top:1px solid #ccc;}
.i_account_list th{height:26px;font-weight:normal;text-align:center}
.i_account_list th i{padding:0px 5px;}
.i_account_list td i{padding:0px 5px;font-size:14px;color:gray;}
.i_account_list td a{text-decoration:underline}
.i_account_list_bg{border-right:1px solid #ccc;padding-left:5px;background-image:url('../img/sh_table_head_bg.png');background-repeat:no-repeat;background-position:left top;}
.i_account_list_left{padding-left:5px;border-right:1px solid #ccc;background-image:url('../img/sh_table_head.png');background-repeat:no-repeat;background-position:left top;}
.i_account_list_right{padding-left:5px;background-image:url('../img/sh_table_head.png');background-repeat:no-repeat;background-position:right top;}
.t1 {background-color:#FFFFFF;text-align:center}
.t2 {background-color:#EDF6FF;text-align:center} 